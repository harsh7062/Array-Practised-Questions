#include <iostream>
using namespace std;

int main() {
    int t;
    cin>>t;
    while(t--){
        int n;
        cin>>n;
        int a[n];
        int i;
        for(i=0;i<n;i++){
            cin>>a[i];
        }
        int sum=0;
        int problem=1000;
        for(i=0;i<n;i++){
            if(a[i]>=problem)
                sum++;
            
            
        }
        cout<<sum<<endl;
    }
	
	return 0;
}
