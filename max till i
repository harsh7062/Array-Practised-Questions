#include<iostream>
using namespace std;
int main(){
	int mx=-1999999999;
	int n;
	cout<<"enter the size of array"<<endl;
	cin>>n;
	
	int array[n];
	cout<<"enter the elements in array";
	for(int i=0;i<n;i++){
		cin>>array[i];
	}
	
	for(int i=0;i<n;i++){
		mx=max(mx,array[i]);
		cout<<mx<<endl;
	}
	
	return 0;
}
