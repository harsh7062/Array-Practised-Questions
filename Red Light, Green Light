#include <iostream>
using namespace std;

int main() {
    int t;
    cin>>t;
    while(t--){
        int n;
        cin>>n;
        int h;
        int count=0;
        cin>>h;
        
        int a[n];
       
        for(int i=0;i<n;i++){
            cin>>a[i];
        }
       
        for(int i=0;i<n;i++){
            if(a[i]>h){
                count++;
            }
        }
        cout<<count<<endl;
    }
	// your code goes here
	return 0;
}
